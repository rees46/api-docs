/* Example of stories implementation in the App */

import React, { useEffect, useState } from 'react';
import { View, Button } from 'react-native';

// We are assuming that SDK had been already initialized

// UI components from SDK
import { StoriesList, StoryViewer } from '<%= config[:rn_sdk_package_code] %>/react-native-sdk';

export default function StoriesExample() {
  const [stories, setStories] = useState([]);
  const [viewerVisible, setViewerVisible] = useState(false);
  const [startIndex, setStartIndex] = useState(0);

  /**
   * Step 1: load stories from API
   */
  useEffect(() => {
    async function loadStories() {
      const response = await sdk.getStories('STORIES_CODE');
      setStories(response?.stories || []);
    }

    loadStories();
  }, []);

  /**
   * Step 2: render UI
   */
  return (
    <View style={{ flex: 1 }}>
      {/* Stories list */}
      <StoriesList
        sdk={sdk}
        code="STORIES_CODE"
        onStoryPress={(story, index) => {
          setStartIndex(index);
          setViewerVisible(true);
        }}
      />

      {/* Optional manual trigger */}
      <Button
        title="Open first story"
        disabled={stories.length === 0}
        onPress={() => {
          setStartIndex(0);
          setViewerVisible(true);
        }}
      />

      {/* Story viewer */}
      <StoryViewer
        visible={viewerVisible}
        stories={stories}
        initialStoryIndex={startIndex}
        onClose={() => setViewerVisible(false)}
        sdk={sdk}
        code="STORIES_CODE"
      />
    </View>
  );
}


